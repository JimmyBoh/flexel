{
  "name": "flexel",
  "version": "0.0.1",
  "description": "Create specialized data structures from level, just like sublevel.",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist/** ./coverage",
    "build": "tsc",
    "watch": "concurrently npm:watch:*",
    "watch:build": "tsc --watch",
    "watch:test": "ava --watch",
    "test": "tsc && nyc --color -a ava -v",
    "debug": "tsc && node ./dist/debug",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyBoh/flexel.git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/flexel/issues"
  },
  "homepage": "https://github.com/JimmyBoh/flexel#readme",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/levelup": "0.0.30",
    "@types/node": "^10.7.1",
    "ava": "^0.25.0",
    "concurrently": "^3.6.1",
    "coveralls": "^3.0.2",
    "nyc": "^12.0.2",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "uuid": "^3.3.2"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "source": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js"
    ]
  }
}
